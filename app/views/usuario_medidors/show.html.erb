<div class="page-header">
  <%= link_to usuario_medidors_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Volver
  <% end %>
  <%= link_to edit_usuario_medidor_path(@usuario_medidor), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h4>Ver usuario_medidor</h4>
</div>

<dl class="dl-horizontal">
  <dt>Usuario:</dt>
  <dd><%= @usuario_medidor.usuario %></dd>

  <dt>Medidor:</dt>
  <dd><%= @usuario_medidor.medidor %></dd>

  <dt>Fecha alta:</dt>
  <dd><%= @usuario_medidor.fecha_alta %></dd>

  <dt>Fecha baja:</dt>
  <dd><%= @usuario_medidor.fecha_baja %></dd>

</dl>
