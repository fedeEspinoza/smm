<div class="page-header">
  <%= link_to novedads_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Volver
  <% end %>
  <%= link_to edit_novedad_path(@novedad), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h4>Ver Novedad</h4>
</div>

<dl class="dl-horizontal">
  <dt>Codigo:</dt>
  <dd><%= @novedad.codigo %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @novedad.descripcion %></dd>

  <dt>Prioridad:</dt>
  <dd><%= @novedad.prioridad %></dd>

</dl>
